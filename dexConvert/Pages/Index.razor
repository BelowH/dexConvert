@page "/"
@using dexConvert.Domains.ApiModels
@inject IApiRepository _apiRepository

<PageTitle>Index</PageTitle>

<h1>Hello, world!</h1>

Welcome to your new app.

<SurveyPrompt Title="How is Blazor working for you?"/>

@code
{

    private async Task LoadManga()
    {
        try
        {
            ApiCollectionResponse _ = await _apiRepository.GetManga("Attack on Titan");
        }
        catch (Exception e)
        {
            Console.WriteLine(e);
            throw;
        }
    }
    
    
}
